<p id="notice"><%= notice %></p>

<h1>Rnamings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rnamings.each do |rnaming| %>
      <tr>
        <td><%= rnaming.area%></td>
        <td><%= link_to 'Show', rnaming_path(rnaming) %></td>
        <td><%= link_to 'Edit', edit_rnaming_path(rnaming) %></td>
        <td><%= link_to 'Destroy', rnaming, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rnaming', new_rnaming_path %>
